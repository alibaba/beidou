{
  "lerna": "3.10.5",
  "version": "2.0.0",
  "changelog": {
    "repo": "alibaba/beidou",
    "labels": {
      "tag: bc": ":boom: Breaking Change",
      "tag: feat": ":rocket: New Feature",
      "tag: fix": ":bug: Bug Fix",
      "tag: polish": ":nail_care: Polish",
      "tag: doc": ":memo: Documentation",
      "tag: internal": ":house: Internal"
    }
  },
  "packages": ["examples/*", "packages/*"],
  "command": {
    "publish": {
      "ignoreChanges": ["*.md", "test/**", "node_modules"],
      "message": "chore(release): publish %s"
    }
  },
  "npmClient": "yarn"
}
