    "beidou-core": "^1.0.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "@types/react": "^16.4.6",
    "@types/webpack": "^4.4.20",
    "egg-ts-helper": "^1.16.1"
  }
