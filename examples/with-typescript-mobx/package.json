{
  "name": "beidou-example-typescript-mobx",
  "version": "2.1.0",
  "description": "beidou project ",
  "scripts": {
    "dev": "npm run asset && beidou dev",
    "start": "beidou dev",
    "lint": "tslint -c tslint.json --fix '**/*.{ts,tsx}' && tsc",
    "prestart": "npm run asset",
    "asset": "node scripts/asset --dev",
    "build": "npm run lint && beidou build",
    "precommit": "lint-staged",
    "debug": "beidou debug"
  },
  "lint-staged": {
    "*.ts": [
      "tslint -c tslint.json --fix '**/*.ts'",
      "git add"
    ],
    "*.tsx": [
      "tslint -c tslint.json --fix '**/*.tsx'",
      "git add"
    ]
  },
  "license": "ISC",
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/history": "^4.7.2",
    "@types/marked": "^0.6.0",
    "@types/prop-types": "^15.5.8",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/sequelize": "^4.27.34",
    "argh": "^0.1.4",
    "cssmin": "^0.4.3",
    "debug": "^3.1.0",
    "deep-assign": "^2.0.0",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "husky": "^1.3.1",
    "lint-staged": "^7.0.0",
    "raf": "^3.4.1",
    "terser": "^3.14.1",
    "tslint": "^5.12.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.4"
  },
  "dependencies": {
    "@alifd/next": "^1.12.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
