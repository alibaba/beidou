    "@babel/polyfill": "^7.2.5",
    "axios": "^0.18.0",
    "beidou": "^2.1.0",
    "beidou-cli": "^2.0.1",
    "classnames": "^2.2.6",
    "egg-sequelize": "^4.3.1",
    "history": "^4.7.2",
    "mobx": "^5.8.0",
    "mobx-react": "^5.4.3",
    "mobx-react-router": "^4.0.5",
    "moment": "^2.23.0",
    "node-sass": "^4.11.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
